<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <env-entry>
        <env-entry-name>mysql_user</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>root</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>mysql_pass</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>noahd</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>jdbc_mysql_address</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>jdbc:mysql://localhost/hspc?useSSL=false</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>subdir</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>C:\Users\noahd\Documents\HSPC\subdir</env-entry-value>
    </env-entry>
</web-app>
